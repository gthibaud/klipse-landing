    "scripts": {
        "dev": "gatsby develop",
        "build": "gatsby build",
        "clean": "gatsby clean",
        "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
        "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
        "start": "npm run dev",
        "test": "npm run type-check && npm run lint",
        "type-check": "tsc"
    },
    "prettier": "@owlgrid-dev/prettier-config",
    "lint-staged": {
        "*.{json,md,css}": [
            "prettier --write"
